{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "image_mobile": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "author_name": {
      "type": "string"
    },
    "publish_date": {
      "type": "date"
    },
    "socialIcons": {
      "type": "component",
      "component": "global.socialicons",
      "repeatable": true
    },
    "content": {
      "type": "blocks"
    },
    "summary": {
      "type": "string"
    },
    "catgeories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::catgeory.catgeory"
    }
  }
}
